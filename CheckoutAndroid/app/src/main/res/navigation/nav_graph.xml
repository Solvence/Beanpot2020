<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.checkout_android.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_gigsFragment"
            app:destination="@id/gigsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_eventsFragment"
            app:destination="@id/eventsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_productsFragment2"
            app:destination="@id/productsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/productsFragment2"
        android:name="com.example.checkout_android.ProductsFragment"
        android:label="fragment_products"
        tools:layout="@layout/fragment_products" />
    <fragment
        android:id="@+id/eventsFragment"
        android:name="com.example.checkout_android.EventsFragment"
        android:label="fragment_events"
        tools:layout="@layout/fragment_events" />
    <fragment
        android:id="@+id/gigsFragment"
        android:name="com.example.checkout_android.GigsFragment"
        android:label="fragment_gigs"
        tools:layout="@layout/fragment_gigs" />
</navigation>